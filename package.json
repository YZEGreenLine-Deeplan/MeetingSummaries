    "package-solution": "gulp package-solution --ship",
    "open-explorer": "start sharepoint\\solution",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./devServices/deploy.service.ps1",
    "serve": "fast-serve"
  },
